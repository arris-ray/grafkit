{
  "name": "arris-ray/grafkit",
  "type": "standard",
  "description": "A toolkit of Grafana utilities.",
  "homepage": "https://github.com/arris-ray/grafkit",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Grafkit\\": "src/"
    }
  },
  "require": {
    "ext-openssl": "*",
    "ext-sqlite3": "*",
    "ext-yaml": "*",
    "ext-zip": "*",
    "guzzlehttp/guzzle": "^7.7",
    "jolicode/castor": "^0.5.2",
    "php": "^8.0",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/discovery": "^1.19",
    "psr/http-client": "^1.0 || ^2.0",
    "psr/http-factory": "^1.0 || ^2.0",
    "psr/http-message": "^1.0 || ^2.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
